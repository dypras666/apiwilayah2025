{
    "name": "sedotphp/api-wilayah-indonesia",
    "description": "RESTful API untuk data wilayah Indonesia (Provinsi, Kabupaten/Kota, Kecamatan, Desa/Kelurahan) - PHP 8 Version",
    "type": "library",
    "keywords": [
        "indonesia",
        "wilayah",
        "api",
        "restful",
        "provinsi",
        "kabupaten",
        "kecamatan",
        "desa",
        "kelurahan",
        "php8"
    ],
    "homepage": "https://github.com/dypras666/apiwilayah2025/",
    "license": "MIT",
    "authors": [
        {
            "name": "SedotPHP",
            "email": "mosys.id@gmail.com",
            "homepage": "https://sedotphp.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "mosys.id@gmail.com",
        "issues": "https://github.com/dypras666/apiwilayah2025/issues",
        "source": "https://github.com/dypras666/apiwilayah2025/"
    },
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "ext-fileinfo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "SedotPHP\\WilayahAPI\\": "src/"
        },
        "files": [
            "config/config.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SedotPHP\\WilayahAPI\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "serve": "php -S localhost:8000 -t .",
        "check-syntax": "find . -name '*.php' -exec php -l {} \;"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "archive": {
        "exclude": [
            "/tests",
            "/coverage",
            "/.gitignore",
            "/phpunit.xml",
            "/logs/*"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    }
}